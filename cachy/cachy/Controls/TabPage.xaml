<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="cachy.Controls.TabPage"
             x:Name="Root">
    <ContentView.Content>
        <Grid>
            <ContentView Content="{Binding TabPageContent, Source={x:Reference Root}}" BackgroundColor="Transparent">
            </ContentView>
        </Grid>
    </ContentView.Content>
</ContentView>